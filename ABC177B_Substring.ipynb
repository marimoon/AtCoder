   "source": [
    "S = input()\n",
    "T = input()\n",
    "\n",
    "def num_unmatch(x, y):\n",
    "  return sum([ True for (i,j) in zip(x,y) if i!=j])\n",
    "\n",
    "ans = len(T)\n",
    "for i in range(len(S)-len(T)+1):\n",
    "  n = num_unmatch(T, S[i:i+len(T)])\n",
    "  if ans > n:\n",
    "    ans = n\n",
    "print(ans)"
   ]
